#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x563bc2fb80f0 .scope module, "datapath_tb" "datapath_tb" 2 2;
 .timescale -9 -12;
v0x563bc2fff2d0_0 .var "clk", 0 0;
v0x563bc2fff370_0 .net "pc", 31 0, v0x563bc2fff060_0;  1 drivers
v0x563bc2fff440_0 .var "rst", 0 0;
S_0x563bc2fb8270 .scope module, "uut" "datapath" 2 8, 3 1 0, S_0x563bc2fb80f0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /OUTPUT 32 "pc"
L_0x7f0c876ac018 .functor BUFT 1, C4<00000000000000000000000000000100>, C4<0>, C4<0>, C4<0>;
v0x563bc2fee840_0 .net/2u *"_s0", 31 0, L_0x7f0c876ac018;  1 drivers
v0x563bc2ffeee0_0 .net "clk", 0 0, v0x563bc2fff2d0_0;  1 drivers
v0x563bc2ffefa0_0 .net "next_pc", 31 0, L_0x563bc300f5b0;  1 drivers
v0x563bc2fff060_0 .var "pc", 31 0;
v0x563bc2fff140_0 .net "rst", 0 0, v0x563bc2fff440_0;  1 drivers
E_0x563bc2feedd0 .event posedge, v0x563bc2fff140_0, v0x563bc2ffeee0_0;
L_0x563bc300f5b0 .arith/sum 32, v0x563bc2fff060_0, L_0x7f0c876ac018;
    .scope S_0x563bc2fb8270;
T_0 ;
    %wait E_0x563bc2feedd0;
    %load/vec4 v0x563bc2fff140_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x563bc2fff060_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x563bc2ffefa0_0;
    %assign/vec4 v0x563bc2fff060_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563bc2fb80f0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x563bc2fff2d0_0;
    %inv;
    %store/vec4 v0x563bc2fff2d0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x563bc2fb80f0;
T_2 ;
    %vpi_call 2 18 "$dumpfile", "datapath.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563bc2fb80f0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563bc2fff2d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563bc2fff440_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563bc2fff440_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "datapath_tb.v";
    "datapath.v";
